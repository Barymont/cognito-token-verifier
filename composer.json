{
    "name": "barymont/jwt-cognito-signature",
    "description": "This package allow verify token from Cognito",
    "version": "0.0.3",
    "autoload": {
        "psr-4": {
            "JwtCognitoSignature\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JwtCognitoSignature\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test" : [
            "php vendor/phpunit/phpunit/phpunit --colors=always ./tests",
            "phpstan  analyse --level=6  ./src/ ./src/"
        ]
    },
    "authors": [
        {
            "name": "Jhonatan Mat√≠as",
            "email": "36485771+seon22break@users.noreply.github.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "firebase/php-jwt": "^6.3"
    },
    "require-dev": {
        "phpstan/phpstan": "1.9.x-dev",
        "phpunit/phpunit": "10.0.x-dev",
        "dg/bypass-finals": "dev-master"
    }
}
